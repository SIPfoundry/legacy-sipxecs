<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:util="http://www.springframework.org/schema/util"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
    http://www.springframework.org/schema/util
    http://www.springframework.org/schema/util/spring-util-2.0.xsd">

  <bean id="unmanagedService" class="org.sipfoundry.sipxconfig.service.UnmanagedService">
    <property name="settingsDao">
      <bean parent="beanWithSettingDao">
        <property name="target">
          <bean parent="beanWithSettingDaoImpl">
            <constructor-arg value="org.sipfoundry.sipxconfig.service.UnmanagedServiceSettings"/>
          </bean>
        </property>
      </bean>
    </property>
  </bean>

  <bean id="unmanagedServiceSettings" class="org.sipfoundry.sipxconfig.service.UnmanagedServiceSettings">
    <property name="modelFilesContext" ref="modelFilesContext" />        
  </bean>

</beans>
