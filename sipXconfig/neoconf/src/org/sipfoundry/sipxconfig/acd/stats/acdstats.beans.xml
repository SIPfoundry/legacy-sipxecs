<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
  
  <bean id="acdStats" class="org.sipfoundry.sipxconfig.acd.stats.AcdStats">
    <property name="settingsDao">
      <bean parent="beanWithSettingDao">
        <property name="target">
          <bean parent="beanWithSettingDaoImpl">
            <constructor-arg value="org.sipfoundry.sipxconfig.acd.stats.AcdStatsSettings"/>
          </bean>
        </property>
      </bean>
    </property>
  </bean>
  
  <bean id="acdStatistics" class="org.sipfoundry.sipxconfig.acd.stats.AcdStatistics">
    <property name="acdContext" ref="acdContext" />
  </bean>
  
  <bean id="acdStatsConfig" class="org.sipfoundry.sipxconfig.acd.stats.AcdStatsConfig">
    <property name="acdStats" ref="acdStats" />
  </bean>

  <bean id="acdStatsSettings" class="org.sipfoundry.sipxconfig.acd.stats.AcdStatsSettings" scope="prototype">
    <property name="modelFilesContext" ref="modelFilesContext"/>
  </bean>

</beans>
