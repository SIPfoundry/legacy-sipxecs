<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.sipfoundry.sipxconfig.phonebook" default-lazy="false">

  <class name="GoogleDomain" table="google_domain">
    <id name="id" column="google_domain_id" type="int" unsaved-value="-1">
      <generator class="sequence">
        <param name="sequence">google_domain_seq</param>
      </generator>
    </id>
	<property name="domainName" column="domain_name"/>
  </class>

</hibernate-mapping>