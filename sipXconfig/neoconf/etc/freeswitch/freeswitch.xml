<?xml version="1.0"?>
<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN"
  "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <group name="freeswitch-config">
    <setting name="FREESWITCH_CODECS">
      <value>PCMU@20i,PCMA@20i,speex,G722,L16</value>
    </setting>
    <setting name="FREESWITCH_SIP_PORT" advanced="yes">
      <type>
        <integer />
      </type>
      <value>15060</value>
    </setting>
    <setting name="FREESWITCH_XMLRPC_PORT" advanced="yes">
      <type>
        <integer />
      </type>
      <value>8080</value>
    </setting>
    <setting name="FREESWITCH_SIP_DEBUG" hidden="yes">
      <type>
        <boolean>
          <true>
            <value>1</value>
          </true>
          <false>
            <value>0</value>
          </false>
        </boolean>
      </type>
      <value>0</value>
    </setting>
    <setting name="MOH_SOURCE">
       <type>
        <enum>
          <option>
            <value>FILES_SRC</value>
          </option>
          <option>
            <value>SOUNDCARD_SRC</value>
          </option>
          <option>
            <value>NONE</value>
          </option>
        </enum>
      </type>
      <value>FILES_SRC</value>
    </setting>
  </group>
</model>
