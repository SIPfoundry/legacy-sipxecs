<?xml version="1.0"?>
<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN" 
  "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <type id="zip">
    <file>
      <contentType>application/zip</contentType>
    </file>
  </type>
  <group name="firmware">
    <label>Grandstream Firmware</label>
    <description>Grandstream Video IP Phones and FXS Gateways.</description>
    <setting name="gxv3000-release.zip">
      <label>GXV 3000 Firmware (zip)</label>
      <type refid="zip"/>
      <description>Firmware. Example file name : Release_GXV3000_1.0.1.27.zip</description>
    </setting>
    <setting name="gxw4004-release.zip">
      <label>GXW 4004 Firmware (zip)</label>
      <type refid="zip"/>
      <description>Firmware. Example file name : Release_GXW4004_1.0.1.54.zip</description>
    </setting>
    <setting name="gxw4008-release.zip">
      <label>GXW 4008 Firmware (zip)</label>
      <type refid="zip"/>
      <description>Firmware. Example file name : Release_GXW4008_1.0.1.54.zip</description>
    </setting>
    <setting name="bt100-ht1-release.zip">
      <label>Budgetone 100 and HT486/286 Firmware (zip)</label>
      <type refid="zip"/>
      <description>Firmware. Example file name : Release_BT100-HT486-HT286_1.1.0.3.zip</description>
    </setting>
    <setting name="bt200-gxp2000-release.zip">
      <label>Budgetone 200 and GXP 2000/2010/2020/1200 Firmware (zip)</label>
      <type refid="zip"/>
      <description>Firmware. Example file name : Release_BT200_GXP1200_GXP2000_GXP2010_GXP2020_1.1.6.16.zip</description>
    </setting>
    <setting name="ht2-release.zip">
      <label>HT488, HT496 and HT386 Firmware (zip)</label>
      <type refid="zip"/>
      <description>Firmware. Example file name : Release_HT488-496-386_1.0.3.96.zip</description>
    </setting>
    <setting name="language-pack.zip">
      <label>GXP 20x0 Language Pack (zip)</label>
      <type refid="zip"/>
      <description>Unpack the file "Language_Pack.zip" and extract the desired language .zip file. The phone only supports one language other than English Example file name : German.zip</description>
    </setting>
  </group>
</model>
