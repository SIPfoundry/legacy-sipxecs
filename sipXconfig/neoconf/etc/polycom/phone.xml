            <integer min="10" max="80" />
          </type>
          <value>20</value>
        </setting>
        <setting name="jitterBufferMin">
          <!-- 40, 50, 60, ... (multiple of 10) -->
          <type>
            <integer min="40" />
          </type>
          <value>40</value>
        </setting>
        <setting name="jitterBufferShrink">
          <!--10, 20, 30, ... (multiple of 10) -->
          <type>
            <integer min="10" />
          </type>
          <value>500</value>
        </setting>
        <setting name="jitterBufferMax">
          <!--  greater than jitterBufferMin, multiple of 10, less than or =500 for IP 500 and IP 600, less than or =
            160 for IP 300 -->
          <type>
            <integer min="10" max="500" />
          </type>
          <value>160</value>
        </setting>
      </group>
      <group name="G711A">
