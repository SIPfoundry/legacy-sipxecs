  <group name="autoconfig">
    <label>Autoconfig</label>
    <setting name="autoconfigOption">
      <label>Autoconfig</label>
      <type>
        <enum>
          <option>
            <value>tftp</value>
          </option>
          <option>
            <value>http</value>
          </option>
          <option>
            <value>https</value>
          </option>
          <option>
            <value>none</value>
          </option>
        </enum>
      </type>
      <value>tftp</value>
      <description>
        Select tftp/http/https protocol for auto provisioning &lt;br/&gt; ( &lt; autoconfig &gt; ) &lt;br/&gt;
      </description>
    </setting>

    <setting name="autoConfigAddress">
      <label>Autoconfig Address</label>
      <type>
        <hostname />
      </type>
      <value />
      <description>
        Domain name or IP address of auto provisioning server &lt;br/&gt; ( &lt; autoConfigAddress &gt; ) &lt;br/&gt;
      </description>
    </setting>

    <setting name="autoConfigProfile">
      <label>Autoconfig Profile</label>
      <type>
        <enum>
          <option>
            <value>Profile-C</value>
          </option>
          <option>
            <value>NONE</value>
          </option>
        </enum>
      </type>
      <value>Profile-C</value>
      <description>
        If Profile-C is selected, the phone uses the profile 'Profile-C'. If NONE is selected auto-configuration is
        disabled &lt;br/&gt; ( &lt; autoConfigProfile &gt; ) &lt;br/&gt;
      </description>
    </setting>

    <setting name="autoConfigPath" hidden="yes">
      <label>Autoconfig Path</label>
      <type>
        <string />
      </type>
      <value>SipTone/config</value>
      <description>Autoconfig Server Path &lt;br/&gt;</description>
    </setting>

    <setting name="autoConfigHttpProxy">
      <label>Autoconfig Http Proxy</label>
      <type>
        <hostname />
      </type>
      <value />
      <description>
        Domain name or IP address of the Http proxy server &lt;br/&gt; ( &lt; autoConfigHttpProxy &gt; ) &lt;br/&gt;
      </description>
    </setting>

    <setting name="autoConfigHttpPort">
      <label>Autoconfig Http Port</label>
      <type>
        <integer max="65536" />
      </type>
      <value />
      <description>
        Port number of the Http proxy server &lt;br/&gt; ( &lt; autoConfigHttpPort &gt; ) &lt;br/&gt;
      </description>
    </setting>

    <setting name="autoConfigEveryBoot">
      <label>Autoconfig Every Boot</label>
      <type refid="on-off" />
      <value>TRUE</value>
      <description>
        If checked, the phone loads the configuration file every time it boots &lt;br/&gt; ( &lt; autoConfigEveryBoot
        &gt; ) &lt;br/&gt;
