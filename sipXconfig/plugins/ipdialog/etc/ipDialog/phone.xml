  <group name="phoneSettings">
    <label>Phone Settings</label>

    <!--
      <setting name="displayname">
      <label>Display Name</label>
      <type>
      <string maxLen="22"/>
      </type>
      <value />
      <description>Display Name ( &lt; displayname &gt; ) &lt;br/&gt; </description>
      </setting>
    -->

    <setting name="dndControl">
      <label>DND</label>
      <type refid="on-off" />
      <value>FALSE</value>
      <description>
        If checked, the phone switches to do-not-disturb mode &lt;br/&gt; ( &lt; dndControl &gt; ) &lt;br/&gt;
      </description>
    </setting>

    <setting name="blinkLightOnIncomingCall">
      <label>Blink Light on Incoming Calls</label>
      <type refid="on-off" />
      <value>TRUE</value>
      <description>
        If checked, the LED lamp on the top of the phone starts blinking on incoming calls &lt;br/&gt; ( &lt;
        blinkLightOnIncomingCall &gt; ) &lt;br/&gt;
      </description>
    </setting>

    <setting name="intercomTimeout">
      <label>Intercom Timeout(sec)</label>
      <type>
        <integer min="0" max="65536" />
      </type>
      <value>15</value>
      <description>
        Intercom timeout is the time in seconds after which an inbound intercom call disconnects. Intercom timeout is
        disabled if this value is set to 0 &lt;br/&gt; ( &lt; intercomTimeout &gt; ) &lt;br/&gt;
      </description>
    </setting>

    <!--
      <setting name="intercomPrefix">
      <label>Intercom Prefix</label>
      <type>
      <string />
      </type>
      <value />
      <description>
      Append Intercom Prefix
      &lt;br/&gt; </description>
      </setting>
      
      <setting name="appendIntercomPrefix">
      <label>Append Intercom Prefix</label>
      <type refid="on-off" />
      <value>FALSE</value>
      <description>
      Append prefix in Intercom Call
      &lt;br/&gt; </description>
      </setting>
      
      <setting name="enableAutologin">
      <label>Enable Auto Login</label>
      <type refid="on-off" />
      <value>TRUE</value>
      <description>
      Automatic Login
      &lt;br/&gt; </description>
      </setting>
    -->

    <setting name="imBeepAlert">
      <label>Enable IM Beep Alert</label>
      <type refid="on-off" />
      <value>TRUE</value>
      <description>
        If checked, the phone beeps when it receives an IM &lt;br/&gt; ( &lt; imBeepAlert &gt; ) &lt;br/&gt;
      </description>
    </setting>

    <!--
      <setting name="natAddress">
      <label>NAT Address</label>
      <type>
      <hostname />
      </type>
      <value />
      <description>
      NAT Address
      &lt;br/&gt; </description>
      </setting>
    -->

    <setting name="syslogServer">
      <label>Syslog Server</label>
      <type>
        <hostname />
      </type>
      <value />
      <description>
        IP address of a syslog server for logging of SIP messages received from and sent to the phone &lt;br/&gt; ( &lt;
        syslogServer &gt; ) &lt;br/&gt;
      </description>
    </setting>

    <setting name="enableSessionTimer">
      <label>Enable Session Timer</label>
      <type refid="on-off" />
      <value>FALSE</value>
      <description>
        If checked, the phone uses a 'keepalive' mechanism in order to better detect whether a call has been
        disconnected &lt;br/&gt; ( &lt; enableSessionTimer &gt; ) &lt;br/&gt;
      </description>
    </setting>

    <setting name="sessionTimerInSec">
      <label>Session Timer(sec)</label>
      <type>
        <integer min="30" max="3600" />
      </type>
      <value>30</value>
      <description>
        Time after which the 'keepalive' is sent &lt;br/&gt; ( &lt; sessionTimerInSec &gt; ) &lt;br/&gt;
      </description>
    </setting>

    <setting name="sessionRefresherFlag">
      <label>Enable Session Refresher Flag</label>
      <type refid="on-off" />
      <value>FALSE</value>
      <description>( &lt; sessionRefresherFlag &gt; ) &lt;br/&gt;</description>
    </setting>

    <setting name="secureAudioScheme">
      <label>Secure Audio Scheme (SRTP)</label>
      <type>
        <enum>
          <option>
            <value>none</value>
          </option>
          <option>
            <value>best effort</value>
          </option>
          <option>
            <value>SRTP Only</value>
          </option>
        </enum>
      </type>
      <value>none</value>
      <description>
        If none is selected, Secure Audio Scheme is disabled. In the best effort scheme phone tries to establish secure
        connection, if it fails then audio is transmitted unsecurely. If SRTP Only scheme is selected then only secure
        audio is allowed &lt;br/&gt; ( &lt; secureAudioScheme &gt; ) &lt;br/&gt;
      </description>
    </setting>

    <setting name="defaultOutgoingLine">
      <label>Default Outgoing Line</label>
      <type>
        <enum>
          <option>
            <value>Line1</value>
          </option>
          <option>
            <value>Line2</value>
          </option>
          <option>
            <value>Last used line</value>
          </option>
        </enum>
      </type>
      <value>Last used line</value>
      <description>
        If Line1 or Line2 is selected, then the selected line is used as default line for making calls. If Last used
        line is selected, then the last active line is used to make outgoing calls &lt;br/&gt; ( &lt;
        defaultOutgoingLine &gt; ) &lt;br/&gt;
      </description>
    </setting>

    <!--
      <setting name="groupPagingPrefix">
      <label>Group Paging Prefix</label>
      <type>
      <integer />
      </type>
      <value></value>
      <description>
      Group Paging Prefix
      &lt;br/&gt; </description>
      </setting>
    -->

    <setting name="hotDeskingAutoLogin">
      <label>Hot Desking Auto Login</label>
      <type refid="on-off" />
      <value>TRUE</value>
      <description>
        If checked, then phone logs in automatically using the User IDs that are configured for Line 1 or Line 2 or both
        &lt;br/&gt; ( &lt; hotDeskingAutoLogin &gt; ) &lt;br/&gt;
      </description>
    </setting>
  </group>

