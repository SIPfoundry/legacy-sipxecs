<model>
  &types;
  <group name="networkSettings">
    <label>Network Settings</label>
    <setting name="networkSetup">
      <label>Network Setup</label>
      <type>
        <enum>
          <option>
            <value>manual</value>
          </option>
          <option>
            <value>dhcp</value>
          </option>
        </enum>
      </type>
      <value>dhcp</value>
      <description>
        If dhcp is selected, the phone use DHCP to get network settings from DHCP server. If manual is selected, the
        phone use network settings defined in fields given below. These network-settings fields have the same meaning as
        the network settings for other network devices and operating systems &lt;br/&gt; ( &lt; networkSettings &gt; )
        &lt;br/&gt;
      </description>
    </setting>

    <setting name="ipAddress">
      <label>IP Address</label>
      <type>
        <ipaddr />
      </type>
      <value></value>
      <description>( &lt; ipAddress &gt; ) &lt;br/&gt;</description>
    </setting>

    <setting name="netmask">
      <label>Netmask</label>
      <type>
        <ipaddr />
      </type>
      <value></value>
      <description>( &lt; netmask &gt; ) &lt;br/&gt;</description>
    </setting>

    <setting name="gateway">
      <label>Gateway</label>
      <type>
        <ipaddr />
      </type>
      <value></value>
      <description>( &lt; gateway &gt; ) &lt;br/&gt;</description>
    </setting>

    <setting name="domainName">
      <label>Domain Name</label>
      <type>
        <hostname />
      </type>
      <value></value>
      <description>( &lt; domainName &gt; ) &lt;br/&gt;</description>
    </setting>

    <setting name="dnsServer1">
      <label>DNS 1</label>
      <type>
        <ipaddr />
      </type>
      <value></value>
      <description>( &lt; dnsServer1 &gt; ) &lt;br/&gt;</description>
    </setting>

    <setting name="dnsServer2">
      <label>DNS 2</label>
      <type>
        <ipaddr />
      </type>
      <value></value>
      <description>( &lt; dnsServer2 &gt; ) &lt;br/&gt;</description>
    </setting>

    <setting name="dnsServer3">
      <label>DNS 3</label>
      <type>
        <ipaddr />
      </type>
      <value></value>
      <description>( &lt; dnsServer3 &gt; ) &lt;br/&gt;</description>
    </setting>

