<?xml version="1.0"?>
<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN"
  "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <type id="loglevel">
    <enum>
      <option>
        <value>DEBUG</value>
      </option>
      <option>
        <value>INFO</value>
      </option>
      <option>
        <value>NOTICE</value>
      </option>
      <option>
        <value>WARNING</value>
      </option>
      <option>
        <value>ERR</value>
      </option>
      <option>
        <value>CRIT</value>
      </option>
      <option>
        <value>ALERT</value>
      </option>
      <option>
        <value>EMERG</value>
      </option>
    </enum>
  </type>
  <type id="nonRootPort">
    <integer min="1025" max="65535"/>
  </type>
  <group name="saa-config">
    <setting name="SIP_SAA_UDP_PORT">
      <type refid="nonRootPort"/>
      <value>5170</value>
    </setting>
    <setting name="SIP_SAA_TCP_PORT">
      <type refid="nonRootPort"/>
      <value>5170</value>
    </setting>
    <setting name="SIP_SAA_LOG_LEVEL">
      <type refid="loglevel" />
      <value>NOTICE</value>
    </setting>
  </group>
</model>