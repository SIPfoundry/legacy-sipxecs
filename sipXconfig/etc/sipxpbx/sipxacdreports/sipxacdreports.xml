<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE model
    PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN"
    "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <type id="enabledDisabled">
    <boolean>
      <true><value>'ENABLED'</value></true>
      <false><value>'DISABLED'</value></false>
    </boolean>
  </type>
  <group name="history">
    <setting name="CONFIG_SERVER_REPORT" hidden="yes">
      <type refid="enabledDisabled"/>
      <value>'ENABLED'</value>
    </setting>
    <setting name="CONFIG_SERVER_AGENT_URL" hidden="yes">
      <type><string/></type>
      <value>'http://localhost:8120'</value>
    </setting>    
    <setting name="LOCATION_FQDN" hidden="yes">
      <type><string/></type>
      <value>'http://localhost:8120'</value>
    </setting>    
  </group>
</model>
