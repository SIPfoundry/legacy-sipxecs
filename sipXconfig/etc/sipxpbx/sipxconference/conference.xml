<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE model 
  PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN"
  "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <type id="yes-no">
    <boolean>
      <true>
        <value>yes</value>
      </true>
      <false>
        <value>no</value>
      </false>
    </boolean>
  </type>
  <group name="fs-conf-conference">
    <setting name="organizer-code" hidden="yes">
      <type>
        <phonepadpin />
      </type>
      <value />
    </setting>
    <setting name="participant-code">
      <type>
        <phonepadpin />
      </type>
      <value />
    </setting>
    <setting name="moderator-code">
      <type>
        <phonepadpin/>
      </type>
      <value />
    </setting>    
    <setting name="organizer_admit" hidden="yes">
      <type>
        <boolean />
      </type>
      <value>0</value>
    </setting>
    <setting name="autorecord" hidden="yes">
      <type>
        <boolean />
      </type>
      <value>0</value>
    </setting>
    <setting name="quickstart">
      <type refid="yes-no" />
      <value>yes</value>
    </setting>
    <setting name="video">
      <type refid="yes-no" />
      <value>no</value>
    </setting>
    <setting name="video-toogle-floor">
      <type refid="yes-no" />
      <value>no</value>
    </setting>
    <setting name="AOR" hidden="yes">
      <value />
    </setting>
    <setting name="ACCESS" hidden="yes">
      <type>
        <enum>
          <option>
            <value>OPEN</value>
          </option>
          <option>
            <value>REMOTE_ADMIT</value>
          </option>
        </enum>
      </type>
      <value>OPEN</value>
    </setting>
    <setting name="MAX_LEGS">
      <type>
        <integer required="yes" />
      </type>
      <value>0</value>
    </setting>
    <setting name="MOH">
       <type>
        <enum>
          <option>
            <value>FILES_SRC</value>
          </option>
          <option>
            <value>SOUNDCARD_SRC</value>
          </option>
          <option>
            <value>NONE</value>
          </option>
        </enum>
      </type>
      <value>FILES_SRC</value>
    </setting>
    <setting name="FS_BRIDGE_CONFERENCE.REMOTE_ADMIT.SECRET" hidden="yes" />
  </group>
  <group name="chat-meeting">
    <setting name="moderated">
      <type>
        <boolean />
      </type>
      <value>0</value>
    </setting>
    <setting name="public">
      <type>
        <boolean />
      </type>
      <value>1</value>
    </setting>
    <setting name="members-only" hidden="yes">
      <type>
        <boolean />
      </type>
      <value>0</value>
    </setting>
  </group>
</model>
