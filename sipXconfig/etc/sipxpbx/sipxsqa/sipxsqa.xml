<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE model 
    PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN" 
    "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <type id="loglevel">
    <enum>
      <option>
        <value>DEBUG</value>
      </option>
      <option>
        <value>INFO</value>
      </option>
      <option>
        <value>NOTICE</value>
      </option>
      <option>
        <value>WARNING</value>
      </option>
      <option>
        <value>ERR</value>
      </option>
      <option>
        <value>CRIT</value>
      </option>
      <option>
        <value>ALERT</value>
      </option>
      <option>
        <value>EMERG</value>
      </option>
    </enum>
  </type>
  <type id="port">
    <integer min="1025" max="65535" />
  </type>
  <group name="sqa-config">
    <setting name="sqa-log-level">
      <type refid="loglevel" />
      <value>NOTICE</value>
    </setting>
    <setting name="sqa-control-port">
      <type refid="port" />
      <value>5240</value>
    </setting>
    <setting name="zmq-subscription-port">
      <type refid="port" />
      <value>5242</value>
    </setting>
  </group>
</model>
