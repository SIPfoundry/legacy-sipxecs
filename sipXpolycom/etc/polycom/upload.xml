<?xml version="1.0"?>
<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN"
  "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <type id="zip">
    <file>
      <contentType>application/zip</contentType>
    </file>
  </type>
  <type id="binary">
    <file>
      <contentType>application/binary</contentType>
    </file>
  </type>
  <group name="firmware">
    <setting name="sip.zip">
      <type>
        <file>
          <contentType>application/zip</contentType>
          <exclude>
            <filename>000000000000.cfg</filename>
            <filename>000000000000-directory~.xml</filename>
            <filename>sip.cfg</filename>
            <filename>phone1.cfg</filename>
          </exclude>
        </file>
      </type>
    </setting>
	<setting name="version">
      <type>
        <enum>
    	  <option><value>3.1.X</value></option>
    	  <option><value>3.2.X</value></option>
    	  <option><value>4.0.X</value></option>
    	  <option><value>4.1.X</value></option>
    	  <option><value>4.1.0</value></option>
    	  <option><value>4.1.2</value></option>
    	  <option><value>4.1.3</value></option>
    	  <option><value>4.1.4</value></option>
    	  <option><value>4.1.5</value></option>
        </enum>
    </type>
    <value>3.2.X</value>
    </setting>  
    <setting name="bootrom">
      <label>Bootrom (zip)</label>
      <type>
        <file>
          <contentType>application/zip</contentType>
          <moveTo>../../</moveTo>
        </file>
      </type>
    </setting>
    <setting name="licenses.zip">
      <type>
	    <file>
	      <contentType>application/zip</contentType>
	      <moveTo>../../</moveTo>
	    </file>
	  </type>
    </setting>
    <setting name="license.cfg">
      <type>
	    <file>
	      <contentType>application/binary</contentType>
	      <moveTo>../../</moveTo>
	    </file>
	  </type>
    </setting>
    <setting name="sip_31x.ld">
      <type>
        <file>
          <contentType>application/binary</contentType>
          <rename>sip.ld</rename>
        </file>
      </type>
    </setting>
    <setting name="allowMultipleUploads" hidden="yes">
      <type>
        <boolean></boolean>
      </type>
      <value>1</value>
    </setting>
  </group>
</model>
