CONFIG_SERVER_HTTPS_PORT=8091
CONFIG_SERVER_SIP_PORT=5090
CONFIG_SERVER_SIP_SECURE_PORT=5091
CONFIG_SERVER_SERVER_SIP_SRV_OR_HOSTPORT=${CONFIG_SERVER_ADDR}:${CONFIG_SERVER_SIP_PORT}
CONFIG_SERVER_AGENT_ADDR=${MY_IP_ADDR}
CONFIG_SERVER_AGENT_PORT=8120

WATCHDOG_SERVER_XMLRPC_PORT=8092

MEDIA_SERVER_ADDR=${MY_IP_ADDR}
MEDIA_SERVER_HTTP_PORT=8090
MEDIA_SERVER_HTTPS_PORT=8091
MEDIA_SERVER_SIP_PORT=5100
MEDIA_SERVER_SIP_SECURE_PORT=5101
MEDIA_SERVER_SIP_SRV_OR_HOSTPORT=${MEDIA_SERVER_ADDR}:${MEDIA_SERVER_SIP_PORT}

STATUS_SERVER_ADDR=${MY_IP_ADDR}
