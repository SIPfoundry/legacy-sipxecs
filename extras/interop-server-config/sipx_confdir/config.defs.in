STATUS_SERVER_HTTPS_PORT=8101
STATUS_SERVER_SIP_PORT=5110
STATUS_SERVER_SIP_SECURE_PORT=5111
STATUS_SERVER_SIP_SRV_OR_HOSTPORT=${STATUS_SERVER_ADDR}:${STATUS_SERVER_SIP_PORT}

VOICEMAIL_SERVER_ADDR=${MY_IP_ADDR}
VOICEMAIL_SERVER_HOSTNAME=${MY_FULL_HOSTNAME}
