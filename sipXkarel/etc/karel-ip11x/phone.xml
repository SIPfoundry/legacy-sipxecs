<!-- <group name="LAN"> -->
  <group name="LAN">
    <profileName>/config/Network/Network.cfg</profileName>
    <setting name="LANTYPE">
      <type>
        <enum>
          <option>
            <value>0</value>
          </option>
          <option>
            <value>1</value>
          </option>
