          <option>
            <value>2</value>
          </option>
        </enum>
      </type>
    </setting>
    <setting name="WANStaticIP">
      <type>
        <string />
      </type>
    </setting>
    <setting name="WANSubnetMask">
      <type>
        <string />
      </type>
    </setting>
    <setting name="WANDefaultGateway">
      <type>
        <string />
      </type>
    </setting>
  </group>
  

