<?xml version="1.0"?>
<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN"
  "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>

<!--
KarelIP11xPhone.java refers to the parameters below

String USER_ID_SETTING = "account/UserName";
String AUTH_USER_ID_SETTING = "account/AuthName";
String DISPLAY_NAME_SETTING = "account/DisplayName";
String PASSWORD_SETTING = "account/password";
String REGISTRATION_SERVER_SETTING = "account/SIPServerHost";
-->

<!-- <group name="account"> -->
  <group name="account">
<!--  <profileName>/config/voip/sipAccount0.cfg</profileName>
      produced dynamically via config.vm; sipAccount0.cfg, sipAccount1.cfg, sipAccount2.cfg
-->
    <setting name="Enable">
      <type>
        <boolean />
      </type>
      <value>1</value>
    </setting>
    <setting name="Label">
      <type>
        <string />
      </type>
    </setting>
    <setting name="DisplayName">
      <type>
        <string />
      </type>
    </setting>
    <setting name="AuthName">
      <type>
        <string />
      </type>
    </setting>
    <setting name="UserName">
      <type>
        <string />
      </type>
    </setting>
    <setting name="password">
      <type>
        <string password="yes" />
      </type>
    </setting>
    <setting name="SIPServerHost">
      <type>
        <string />
      </type>
    </setting>
    <setting name="SIPServerPort">
      <type>
        <integer />
      </type>
      <value>5060</value>
    </setting>
    <setting name="Transport">
      <type>
        <enum>
          <option>
            <value>0</value>
          </option>
          <option>
            <value>1</value>
          </option>
          <option>
            <value>2</value>
          </option>
          <option>
            <value>3</value>
          </option>
        </enum>
      </type>
      <value>3</value>
    </setting>
    <setting name="AnonymousCall">
      <type>
        <boolean />
      </type>
      <value>0</value>
    </setting>
    <setting name="RejectAnonymousCall">
      <type>
        <boolean />
      </type>
      <value>0</value>
    </setting>
    <setting name="Expire">
      <type>
        <integer min="60" max="3600" />
      </type>
      <value>3600</value>
    </setting>
    <setting name="SIPListenPort">
      <type>
        <integer />
      </type>
      <value>5060</value>
    </setting>
    <setting name="SubscribeRegister">
      <type>
        <boolean />
      </type>
      <value>0</value>
    </setting>
    <setting name="SubscribeMWI">
      <type>
        <boolean />
      </type>
      <value>1</value>
    </setting>
    <setting name="SubscribeMWIExpire">
      <type>
        <integer min="0" max="84600" />
      </type>
      <value>3600</value>
    </setting>
    <setting name="CIDSource">
      <type>
        <enum>
          <option>
            <value>0</value>
          </option>
          <option>
            <value>1</value>
          </option>
        </enum>
      </type>
      <value>0</value>
    </setting>

    <setting name="Enable_100Rel">
      <type>
        <boolean />
      </type>
      <value>0</value>
    </setting>

    <setting name="srtp_encryption">
      <type>
        <boolean />
      </type>
      <value>0</value>
    </setting>

    <setting name="MusicServerUri">
      <type>
        <string />
      </type>
    </setting>
  </group>


  <!-- <group name="DTMF"> -->
  <group name="DTMF">
    <setting name="DTMFInbandTransfer">
      <type>
        <enum>
          <option>
            <value>0</value>
          </option>
          <option>
            <value>1</value>
          </option>
          <option>
            <value>2</value>
          </option>
          <option>
            <value>3</value>
          </option>
        </enum>
      </type>
      <value>1</value>
    </setting>

  <setting name="InfoType">
      <type>
        <enum>
          <option>
            <value>0</value>
          </option>
          <option>
            <value>1</value>
          </option>
          <option>
            <value>2</value>
          </option>
          <option>
            <value>3</value>
          </option>
        </enum>
      </type>
      <value>0</value>
    </setting>

    <setting name="DTMFPayload">
      <type>
        <integer min="96" max="255" />
      </type>
      <value>100</value>
    </setting>
  </group>


<!-- <group name="NAT"> -->
  <group name="NAT">
    <setting name="NATTraversal">
      <type>
        <boolean />
      </type>
      <value>0</value>
    </setting>

    <setting name="STUNServer">
      <type>
        <string />
      </type>
    </setting>

    <setting name="STUNPort">
      <type>
        <integer />
      </type>
      <value>3478</value>
    </setting>

    <setting name="EnableUDPUpdate">
      <type>
        <boolean />
      </type>
      <value>1</value>
    </setting>

    <setting name="UDPUpdateTime">
      <type>
        <integer min="1" max="90" />
      </type>
      <value>30</value>
    </setting>

    <setting name="rport">
      <type>
        <boolean />
      </type>
      <value>0</value>
    </setting>
  </group>

  
