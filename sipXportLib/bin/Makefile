################################################################
# localize and install process definition based on:
# SIPX_PROCESS_DEFS : the process definition file name without '-process.xml'

install-data-hook: \
	$(foreach proc,$(SIPX_PROCESS_DEFS),$(DESTDIR)$(SIPX_DATADIR)/process.d/$(proc)-process.xml)

$(foreach proc,$(SIPX_PROCESS_DEFS),$(DESTDIR)$(SIPX_DATADIR)/process.d/$(proc)-process.xml) \
	: $(DESTDIR)$(SIPX_DATADIR)/process.d/% : %
	$(INSTALL) -D -m 644 $< $@

$(foreach proc,$(SIPX_PROCESS_DEFS),$(proc)-process.xml) : % : %.in
	@echo "Localize process definition: $*"
	@$(LocalizeSipXconfig) $< > $@

.PHONY: $(foreach proc,$(SIPX_PROCESS_DEFS),$(proc)-process.xml)

################################################################
# Common pattern rules

# localize and syntax check a python file
%.py : %.py.in
	@echo "Localize $< -> $@"
	@$(LocalizeSipXconfig) $< > $@
	chmod +x $@
	python -m py_compile $@

# localize and syntax check a perl file
%.pl : %.pl.in
	@echo "Localize $< -> $@"
	@$(LocalizeSipXconfig) $< > $@
	chmod +x $@
	perl -wc $@

install-exec-hook :

sipx-upgrade: $(srcdir)/sipx-upgrade.in
	@echo "Localize $< -> $@"
	@$(LocalizeSipXconfig) $(srcdir)/sipx-upgrade.in > sipx-upgrade

shell-unit: $(srcdir)/shell-unit.in
	cp $(srcdir)/shell-unit.in shell-unit

$(DESTDIR)$(bindir)/sipx-upgrade: sipx-upgrade
	$(INSTALL) -D -m 755 $(srcdir)/sipx-upgrade $(DESTDIR)$(bindir)/sipx-upgrade

sipx-upgrade-testsuite: sipx-upgrade shell-unit $(srcdir)/sipx-upgrade-testsuite.in
	@echo "Localize $< -> $@"
	@$(LocalizeSipXconfig) $(srcdir)/sipx-upgrade-testsuite.in > $@
	chmod +x $@

# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
